<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nightmare.jli.myslidingfinish">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:theme="@style/MyTheme_Transparent"
            android:name="com.nightmare.jli.myslidingfinish.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/MyTheme_Transparent"
            android:name="com.nightmare.jli.myslidingfinish.AbsActivity" >
        </activity>

        <activity
            android:theme="@style/MyTheme_Transparent"
            android:name="com.nightmare.jli.myslidingfinish.ViewPagerActivity"></activity>
    </application>

</manifest>